{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "repo_tags": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "repo_digests": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "top_layer": {
            "type": "string"
        },
        "size": {
            "type": "uint64"
        },
        "created": {
            "type": "string"
        },
        "loaded": {
            "type": "string"
        },
        "uid": {
            "type": "object",
            "properties": {
                "value": {
                    "type": "int64"
                }
            }
        },
        "username": {
            "type": "string"
        },
        "Labels": {
            "oneOf": [{
                    "$ref": "../defs.json#/definitions/mapStringString"
                },
                {
                    "type": "null"
                }
            ]
        }
    }
}